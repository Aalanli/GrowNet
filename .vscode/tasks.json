{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "run",
			"args": ["--package", "grownet_ui", "--bin", "grownet_ui"],
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: cargo run grownet-models",
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"type": "shell",
			"label": "clean config",
			"command": "rm -r assets/config"
		}
	]
}